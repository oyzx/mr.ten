/* pages/user/user.wxss */
page {
  width: 100%;
  height: 100%;
  background-color: #fff;
}
/* 头部  */
.header {
  width: 100%;
  height: 2.5rem;
  display: flex;
  flex-direction: row;
  align-items: center;
  background-color: #FFF100;
  position: fixed;
  z-index: 2;
}
.header .headers {
  display: flex;
  flex-direction: row;
  justify-content: center;
}
.header .header-titile {
  width:75%;
}
.header .header-icon {
  width:15%;
}
.header .header-icon image {
  width:25px;
  height:25px;
}